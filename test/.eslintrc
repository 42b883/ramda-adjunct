{
  "env": {
    "mocha": true
  },
  "globals": {
    "document": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-void": 0,
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "no-array-constructor": 0,
    "prefer-rest-params": 0,
    "no-new-wrappers": 0
  }
}
